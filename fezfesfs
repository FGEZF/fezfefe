local Material = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/MaterialLua/master/Module.lua"))()

local X = Material.Load({
	Title = "Sanic GUI / Dodge#5540",
	Style = 3,
	SizeX = 500,
	SizeY = 350,
	Theme = "Dark",
	ColorOverrides = {
		MainFrame = Color3.fromRGB(235,235,235)
	}
})

local Y = X.New({
	Title = "Sanic Infection GUI By Dodge#5540"
})
local A = Y.Button({
	Text = "Become Yellow Sanic",
	Callback = function()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.workspace.GameFolder.InfectionPart.Strong.TransfurPart.CFrame
wait(1)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-3.13, 4.999, 85.69)
	end,
	Menu = {
		Information = function(self)
			X.Banner({
				Text = "This function can get you banned in up-to-date servers; use at your own risk."
			})
		end
	
	}
})

